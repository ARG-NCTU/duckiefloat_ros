<?xml version="1.0" ?>

<launch>

    <arg name="sim" default="True" />

    <node pkg="joy" type="joy_node" name="joy_node">
        <param name="autorepeat_rate" value="20" />
    </node>

    <include file="$(find subt_rl)/launch/pcToLaser_240_duckieboat.launch">
        <arg name="veh" value="boat1" />
    </include>
    <group ns="boat1">
        <node pkg="subt_rl" type="teleop.py" name="teleop" />
        <node pkg="subt_rl" type="run_rdpg.py" name="run_rdpg" output="screen">
            <param name="model" value="subt_rl495.ckpt-1037" />
            <param name="sim" value="$(arg sim)" />
        </node>
    </group>

    <include file="$(find subt_rl)/launch/pcToLaser_240_duckieboat.launch">
        <arg name="veh" value="boat2" />
    </include>
    <group ns="boat2">
        <node pkg="subt_rl" type="teleop.py" name="teleop" />
        <node pkg="subt_rl" type="run_rdpg.py" name="run_rdpg" output="screen">
            <param name="model" value="subt_rl495.ckpt-1037" />
            <param name="sim" value="$(arg sim)" />
        </node>
    </group>

    <include file="$(find subt_rl)/launch/pcToLaser_240_duckieboat.launch">
        <arg name="veh" value="boat3" />
    </include>
    <group ns="boat3">
        <node pkg="subt_rl" type="teleop.py" name="teleop" />
        <node pkg="subt_rl" type="run_rdpg.py" name="run_rdpg" output="screen">
            <param name="model" value="subt_rl495.ckpt-1037" />
            <param name="sim" value="$(arg sim)" />
        </node>
    </group>

    <include file="$(find subt_rl)/launch/pcToLaser_240_duckieboat.launch">
        <arg name="veh" value="boat4" />
    </include>
    <group ns="boat4">
        <node pkg="subt_rl" type="teleop.py" name="teleop" />
        <node pkg="subt_rl" type="run_rdpg.py" name="run_rdpg" output="screen">
            <param name="model" value="subt_rl495.ckpt-1037" />
            <param name="sim" value="$(arg sim)" />
        </node>
    </group>


</launch>